     name : _RechercheRuban_AfficheResultat
     internal_properties : CAAAAAgAAACrosG+d3EmDUVv2d0UAzOLeguOzyE5c5JAyx+nJcJzEqJUC7kuJztwzWMqNgCASXLosHHUhfr7rb2cJTFxD614S5D5EKZtaAqIURg0PcmCQjUZ74XVjI2vw24RYuDlIePQYruXrzBsQb2Fk7Ic39SIuCNYSmycrnDLiTNHsOYc
     procedure_id : 1268757459786670086

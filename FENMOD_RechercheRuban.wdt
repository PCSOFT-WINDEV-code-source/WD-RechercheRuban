   -
     internal_properties : CAAAAAgAAABTJUUdw9Q+BtHadziMh1Y/sSvePIaX9wtZ0MvGGEXpOQtmGPjV8nVxUokf5gxym3yhnMjEhroh9bVcH5CtA/9jSoKD9pbLYzUfUfknq31BkfB6Qxq/rlVEj+dMRwz8TL/eev6UM8q9S+t+r/Q=
     code : |1+
      
      EXTERN "WINCONST.WL"
      
      ETypeElement est une enumeration
      	eltInconnu			// Elément de type inconnu
      	eltVolet			// Volet
      	eltRegroupement		// Regroupement
      	eltChamp			// Champ
      FIN
      
      // Structure qui contient les informations relatives aux différents éléments (champs, regroupements, volets) du ruban
      STElementRuban est une structure
      	sNomCompletChamp 		est une chaine						// Nom complet de l'élément (regroupement/champ= ou numéro du volet
      	sCheminAccesRuban 		est une chaine						// Chemin d'accès au ruban (texte préformaté)
      	sLibellé 				est une chaine						// Libellé de l'élément
      	sBulleAide 				est une chaine						// Bulle d'aide de l'élément
      	sTexteRecherche 		est une chaine						// Texte utilisé pour la recherche (correspond à la concaténation du libellé et de la bulle)
      	nIndiceVolet 			est un entier						// Indice du volet du champ / regroupement
      	sRegroupementParent		est une chaine						// Nom complet du regroupement
      	eType					est un ETypeElement					// Type de l'élément (volet, regroupement ou champ)
      FIN
      
      // Variables du modèle de fenêtres
      gsImageVoletInitial			est une chaine						// Image initialement utilisée pour les volets du ruban
      gsNomChampRuban				est une chaine						// Nom du champ ruban manipulé  (cette variable doit être initialisée dans la fenêtre qui utilise le modèle)
      gtabElementsRuban 			est un tableau de STElementRuban	// Tableau qui contient tous les éléments d'un ruban
      gtabElementsRubanRech 		est un tableau de STElementRuban	// Tableau qui contient les éléments filtrés
   -

   -
     name : _RechercheRuban_DéfinitImageVolet
     internal_properties : 
     procedure_id : 1268750622204089539
     type_code : 14
     code : |1+
      // Résumé : Crée une image de volet à partir de l'image originale en appliquant une couleur
      // Syntaxe :
      //[ <Résultat> = ] _RechercheRuban_DéfinitImageVolet (<nCouleur> est entier)
      //
      // Paramètres :
      //	nCouleur (entier) : Couleur à appliquer
      // Valeur de retour :
      // 	chaîne ANSI : Chemin du fichier image résultat
      //// Traitement automatique des erreurs : RENVOYER Chaine vide "
      //
      
      PROCEDURE PRIVEE _RechercheRuban_DéfinitImageVolet(LOCAL nCouleur est un entier) : chaine
      
      // Définit le fichier image temporaire
      sImageTemporaire est une chaine = fFichierTemp+".png"
      
      // Applique la couleur en fond de l'image finale
      IMG_ImageVoletFinal..CouleurFond = nCouleur
      
      // Recopie l'image du volet dans le champ image
      dCopieImage(IMG_ImageVolet, IMG_ImageVoletFinal, copieSrcEt)
      
      // Mémorise le dessin dans un fichier temporaire
      dSauveImagePNG(IMG_ImageVoletFinal, sImageTemporaire)
      
      // Fin du dessin
      dFinDessin(IMG_ImageVoletFinal)
      
      
      // Chemin du fichier résultat
      renvoyer sImageTemporaire
     type : 458752
   -
     name : RechercheRuban_Affiche
     internal_properties : CAAAAAgAAACOI1tHdG/6v238bPZXaR8cB89P9LOeZT6gHnJPJfbFlDUg14wPm9qkzPfrcWnrseFeSnuHz7OnYjChBRs6Z2Zn7a4jIjewhxaG3eVf0Cq2ohkE71Hr3DueK8ZwuNG0CR3yyC9ykNWsZytWMAXLz5VckqXZ7BdqyOSv48+h78M5
     procedure_id : 1268751055997067577
     type_code : 14
     group : 4
     code : |1+
      // Résumé : Affiche ou cache les champs liés à la recherche dans le ruban
      // Syntaxe :
      //RechercheRuban_Affiche ( [<bAffiche> est booléen])
      //
      // Paramètres :
      //	bAffiche (booléen) : Vrai pour afficher les éléments, Faux pour les cacher. Si le paramètre n'est pas précisé, la visibilité courante est inversée
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RechercheRuban_Affiche(LOCAL bAffiche est un booleen = PAS GR_RubanRecherche..Visible)
      
      
      // S'il faut afficher les champs
      si bAffiche ALORS
      	
      	// Affiche les champs de recherche (image de fond, saisie, zone répétée, ...)
      	GR_RubanRecherche..Visible = vrai
      	
      	// Exécute la recherche (par défaut, le champ de saisie est en mémorisation et contient donc la dernière valeur saisie)
      	ExécuteTraitement(SAI_RechercheCommandeRuban, trtModification)
      	
      	// Reprise de la saisie
      	RepriseSaisie(SAI_RechercheCommandeRuban)
      	
      	
      SINON // S'il faut cacher les champs
      	
      	// Cache les champs de recherche (image de fond, saisie, zone répétée, ...)
      	GR_RubanRecherche..Visible 			= Faux
      	
      	// Cache l'image et la forme de superposition
      	IMG_MiseEnEvidence_Fond..Visible 	= Faux
      	FRM_MiseEnEvidence_Cadre..Visible 	= Faux
      	
      	// Ré-applique l'image initiale de volet 
      	si champexiste(gsNomChampRuban) alors
      		{gsNomChampRuban, indchamp}..Image	= gsImageVoletInitial
      	Fin
      	
      FIn
     type : 458752

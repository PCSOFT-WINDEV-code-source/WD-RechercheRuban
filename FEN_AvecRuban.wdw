     code : |1-
      // Résumé : Procédure appelée lors d'un clic gauche sur la fenêtre
      // Syntaxe :
      //_RechercheRuban_ClicGaucheFenêtre ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PRIVE _RechercheRuban_ClicGaucheFenêtre()
      
      // Si le groupe de recherche est visible
      SI GR_RubanRecherche..Visible ALORS
      	
      	// Si le clic a lieu ailleurs que sur la zone dédiée à la recherche dans le ruban
      	SI SourisPosY(spfenetre) < IMG_FondRecherche..Y _OU_ SourisPosY(spFenêtre) > IMG_FondRecherche..Y+IMG_FondRecherche..Hauteur ALORS
      		
      		// Cache les éléments de la recherche (l'application revient en utilisation normale)
      		RechercheRuban_Affiche(faux)
      		
      	FIN
      	
      FIN
     type : 458752
   -
     name : _RechercheRuban_MiseEnEvidenceChamp
     internal_properties : 
     procedure_id : 1268751055997198665
     type_code : 14
     code : |1-
      // Résumé : Met en évidence le champ, regroupement ou volet du résultat de recherche sélectionné
      // Syntaxe :
      //_RechercheRuban_MiseEnEvidenceChamp (<stUnElement> est STElementRuban)
      //
      // Paramètres :
      //	stUnElement (STElementRuban) : Structure contenant les informations du champ à  mettre en évidence
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PRIVE _RechercheRuban_MiseEnEvidenceChamp(stUnElement est un STElementRuban)
      
      // Le champ Ruban doit exister
      si champexiste(gsNomChampRuban) = faux alors retour
      
      // Positionne le ruban sur le volet
      {gsNomChampRuban, indchamp} = stUnElement.nIndiceVolet
      
      // Cache les champs qui permettent la mise en évidence des champs et regroupements
      IMG_MiseEnEvidence_Fond..visible 	= faux
      FRM_MiseEnEvidence_Cadre..visible 	= faux
      
      // Si le résultat à mettre en évidence est un regroupement ou un champ
      SI stUnElement.eType dans (eltChamp, eltregroupement) ALORS
      	
      	// Superpose l'image de mise en évidence
      	IMG_MiseEnEvidence_Fond..Largeur 	= {stUnElement.sNomCompletChamp, indChamp}..Largeur
      	IMG_MiseEnEvidence_Fond..Hauteur 	= {stUnElement.sNomCompletChamp, indChamp}..Hauteur
      	IMG_MiseEnEvidence_Fond..X 			= {stUnElement.sNomCompletChamp, indChamp}..X
      	IMG_MiseEnEvidence_Fond..Y 			= {stUnElement.sNomCompletChamp, indChamp}..Y
      	
      	// Encadre le champ ou regroupement
      	FRM_MiseEnEvidence_Cadre..Largeur 	= {stUnElement.sNomCompletChamp, indChamp}..Largeur+2
      	FRM_MiseEnEvidence_Cadre..Hauteur 	= {stUnElement.sNomCompletChamp, indChamp}..Hauteur+2
      	FRM_MiseEnEvidence_Cadre..X 		= {stUnElement.sNomCompletChamp, indChamp}..X-1
      	FRM_MiseEnEvidence_Cadre..Y 		= {stUnElement.sNomCompletChamp, indChamp}..Y-1
      	
      	// Affiche les champs qui permettent la mise en évidence (l'image pour le fond et la forme pour le cadre)
      	IMG_MiseEnEvidence_Fond..Visible 	= Vrai
      	FRM_MiseEnEvidence_Cadre..Visible 	= Vrai
      	
      FIN
      
      // Fait passer l'image, le cadre et le fond du jaune au rouge ...
      POUR i = 0 _a_ IMG_DégradéRechRuban..Largeur-1
      	{gsNomChampRuban, indChamp}..image 		= _RechercheRuban_DéfinitImageVolet(dPixelCouleur(IMG_DégradéRechRuban, i, 5))
      	IMG_MiseEnEvidence_Fond..CouleurFond	= dPixelCouleur(IMG_DégradéRechRuban, i, 5)
      	FRM_MiseEnEvidence_Cadre..Couleur		= dPixelCouleur(IMG_DégradéRechRuban, i, 5)
      	Temporisation(0, tempoSourisEtClavier)
      FIN
      
      // ... puis du rouge au jaune
      POUR i = IMG_DégradéRechRuban..Largeur-1 _a_ 0 pas -1
      	{gsNomChampRuban, indChamp}..Image 		= _RechercheRuban_DéfinitImageVolet(dPixelCouleur(IMG_DégradéRechRuban, i, 5))
      	IMG_MiseEnEvidence_Fond..CouleurFond	= dPixelCouleur(IMG_DégradéRechRuban, i, 5)
      	FRM_MiseEnEvidence_Cadre..Couleur		= dPixelCouleur(IMG_DégradéRechRuban, i, 5)
      	Temporisation(0, tempoSourisEtClavier)
      FIN
      
      // Force un rafraichissement 
      si GR_RubanRecherche..visible = faux alors
      	
      	// Cache l'image et la forme de superposition
      	IMG_MiseEnEvidence_Fond..Visible 	= Faux
      	FRM_MiseEnEvidence_Cadre..Visible 	= Faux
      	
      	// Ré-applique l'image initiale de volet 
      	{gsNomChampRuban, indChamp}..Image 	= gsImageVoletInitial
      	
      Fin

     name : _RechercheRuban_ClicGaucheFenêtre
     internal_properties : CAAAAAgAAACSWNxFJRNfr0VfgGzRIh1eH8JmVJeUur4vygWrBeXNLMhAH62gWOIzTgDvfT8fCIYmFyo7WbLofz8Z+IuHOhvwP/exgtL2QT90FGjT0iYMKDb8TdjIsOzQJjX0wJ8/E9bqULoBDDWIpzTzbjU=
     procedure_id : 1268751055997133129

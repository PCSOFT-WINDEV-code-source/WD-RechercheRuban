     name : _RechercheRuban_AfficheResultat
     internal_properties : CAAAAAgAAAAvMGht0GGXMCjBVZup0kqBDDxL38iXhsQGEKHEbLZ+T3WVIMKLSza6/LoAfZPJko6B2zuVKm0BE4MFQCEHsKY9ZqifVd/odBQHzRv62B9PDorA9aqQayxilI4CRt8YWrAIA+hJQsAFFngoLCRXC/lKtZixnr/rwoaYZ8+wy4o36XHke2yQmo+uEP/Lwp5n0kwfZiwagPEsW4nTd7FmoLIyIgMLdUxI0V7ed43h4adagLOXC1grQ8JNKJQarfOsamM9GSrN
     procedure_id : 1268758224292009873
     type_code : 14
     code : |1+
      // Résumé : Affiche les résultats d'une recherche 
      // Syntaxe :
      //_RechercheRuban_AfficheResultat ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //// Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code, avec un différé de 1 centièmes de seconde
      // 
      
      PROCEDURE _RechercheRuban_AfficheResultat()
      
      sLibelleFormaté 	est une chaine
      sBulleFormatée 		est une chaine
      
      
      // Supprime tous les champs de la recherche précédente
      SupprimeTout(gtabElementsRubanRech)
      
      // Vide la zone répétée
      ZoneRépétéeSupprimeTout(ZR_ResultatRecherche)
      
      // Pour chaque champ du ruban
      POUR TOUT element stUnChamp de gtabElementsRuban
      	
      	// Si le terme de la recherche est présent pour ce champ
      	SI Position(stUnChamp.sTexteRecherche, SAI_RechercheCommandeRuban, 1, SansCasse) > 0 ALORS
      		
      		// Il ne faut prendre en compte que les éléments visibles
      		Si _RechercheRuban_ElementEstVisible(stUnChamp) alors
      			
      			// Mémorise ce champ comme résultat de recherche
      			Ajoute(gtabElementsRubanRech, stUnChamp)
      			
      			// Formate le libellé (pour mettre en évidence le terme recherché)
      			sLibelleFormaté = _RechercheRuban_SurligneTexte(stUnChamp.sLibellé, SAI_RechercheCommandeRuban, LIB_LibelléChamp..NomComplet)
      			
      			// Formate la bulle (pour mettre en évidence le terme recherché)
      			sBulleFormatée = _RechercheRuban_SurligneTexte(stUnChamp.sBulleAide, SAI_RechercheCommandeRuban, LIB_BulleAideChamp..NomComplet)
      			
      			// Ajoute une ligne dans la zone répétée
      			ZoneRépétéeAjouteLigne(ZR_ResultatRecherche, sLibelleFormaté, sBulleFormatée, stUnChamp.sCheminAccesRuban)
      			
      		FIN
      		
      	FIN
      	
      FIN
      
      // Ne rien sélectionner par défaut
      ZoneRépétéeSelectMoins(ZR_ResultatRecherche)
      
      // Modifie la hauteur en fonction du nombre de résultat
      nNbLigne est un entier 			= min(ZR_ResultatRecherche..Occurrence, 5)
      // La hauteur du champ Zone répétée correspond à la hauteur d'une ligne + 1 (pour le trait de séparation) * le nombre de lignes à afficher (5 max)
      ZR_ResultatRecherche..Hauteur 	= (ZR_ResultatRecherche..HauteurLigne+1) * nNbLigne
      // La hauteur du champ Image de fond correspond à sa hauteur initiale + la hauteur occupée par les lignes supplémentaires de la zone répétée
      IMG_FondRecherche..Hauteur 		= IMG_FondRecherche..HauteurInitiale + (ZR_ResultatRecherche..HauteurLigne+1) * (nNbLigne-1)
     type : 458752
   -
     name : _RechercheRuban_Initialise
     internal_properties : 
     procedure_id : 1269787307037305108
     type_code : 14
     code : |1-
      // Résumé : Charge tous les champs d'un ruban
      // Syntaxe :
      //[ <Résultat> = ] _RechercheRuban_Initialise ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : Vrai si le chargement est correct, Faux autrement
      //
      // Traitement automatique des erreurs : RENVOYER FAUX
      //
      
      PROCEDURE PRIVE _RechercheRuban_Initialise() : booleen
      
      stUnElement 			est un STElementRuban
      sNomRegroupement 		est une chaine
      sNomChamp				est une chaine
      sLibelléVolet			est une chaine
      sLibelléRegroupement	est une chaine
      sLibelléChamp			est une chaine
      
      
      // Le nom du champ Ruban doit être correct
      si champexiste(gsNomChampRuban) = faux alors
      	erreurconstruit("Le nom du champ ruban renseigné (""%1"") est invalide." +RC+ "Vous devez renseigner la variable gsNomChampRuban du modèle de fenêtres", gsnomchampruban)
      	renvoyer faux
      FIN
      
      // Vide la mémorisation précédente
      supprimetout(gtabElementsRuban)
      supprimetout(gtabElementsRubanRech)
      
      // Parcourt les volets du ruban
      POUR nIndiceVolet = 1 _a_ {gsNomChampRuban, indchamp}..Occurrence
      	
      	// Mémorise le libellé du volet (ou l'indice du volet si le libellé est vide)
      	sLibelléVolet = ({gsNomChampRuban, indChamp}[nIndiceVolet]..Libellé <> "" ? {gsNomChampRuban, indChamp}[nIndiceVolet]..Libellé SINOn nIndiceVolet)
      	
      	// Mémorise le volet comme un champ du ruban
      	VariableRAZ(stUnElement)
      	stUnElement.sNomCompletChamp 	= nIndiceVolet
      	stUnElement.sLibellé 			= sLibelléVolet
      	stUnElement.sBulleAide 			= {gsNomChampRuban, indChamp}[nIndiceVolet]..Bulle
      	stUnElement.sTexteRecherche 	= stUnElement.sLibellé +" "+ stUnElement.sBulleAide
      	stUnElement.sCheminAccesRuban 	= ChaîneConstruit("Ruban >>> Volet ""%1""", sLibelléVolet)
      	stUnElement.nIndiceVolet 		= nIndiceVolet
      	stUnElement.eType				= eltVolet
      	Ajoute(gtabElementsRuban, stUnElement)
      	
      	// Parcourt les regroupements du volet
      	sNomRegroupement = EnumèreSousElément(gsNomChampRuban+"."+nIndiceVolet, enumPremier)
      	TANTQUE sNomRegroupement <> ""
      		
      		// Si l'élément n'est pas un regroupement
      		si {sNomRegroupement, indchamp}..type <> typRegroupementRuban alors continuer
      		
      		// Mémorise le libellé du regroupement (ou le nom du regroupement si le libellé est vide)
      		sLibelléRegroupement = ({sNomRegroupement, indChamp}..Libellé <> "" ? {sNomRegroupement, indChamp}..Libellé SINON {sNomRegroupement, indChamp}..nom)
      		
      		// Mémorise le regroupement comme un champ du ruban
      		VariableRAZ(stUnElement)
      		stUnElement.sNomCompletChamp 	= {sNomRegroupement, indChamp}..NomComplet
      		stUnElement.sLibellé 			= sLibelléRegroupement
      		stUnElement.sBulleAide 			= {sNomRegroupement, indChamp}..Bulle
      		stUnElement.sTexteRecherche 	= stUnElement.sLibellé +" "+ stUnElement.sBulleAide
      		stUnElement.sCheminAccesRuban 	= ChaîneConstruit("Ruban >>> Volet ""%1"" >>> Regroupement ""%2""", sLibelléVolet, sLibelléRegroupement)
      		stUnElement.nIndiceVolet 		= nIndiceVolet
      		stUnElement.eType				= eltRegroupement
      		Ajoute(gtabElementsRuban, stUnElement)
      		
      		// Parcourt les champs du regroupement
      		sNomChamp = EnumèreSousElément(gsNomChampRuban+"."+nIndiceVolet+"."+sNomRegroupement, enumPremier)
      		TANTQUE sNomChamp <> ""
      			
      			// Mémorise le libellé du champ (ou le nom du champ si le libellé est vide)
      			sLibelléChamp = ({sNomChamp, indChamp}..Libellé <> "" ? {sNomChamp, indChamp}..Libellé SINON {sNomChamp, indChamp}..Nom)
      			
      			// Mémorise le champ
      			VariableRAZ(stUnElement)
      			stUnElement.sNomCompletChamp 	= {sNomChamp, indChamp}..NomComplet
      			stUnElement.sLibellé 			= {sNomChamp, indChamp}..Libellé
      			stUnElement.sBulleAide 			= {sNomChamp, indChamp}..Bulle
      			stUnElement.sTexteRecherche 	= stUnElement.sLibellé +" "+ stUnElement.sBulleAide
      			stUnElement.sCheminAccesRuban 	= ChaîneConstruit("Ruban >>> Volet ""%1"" >>> Regroupement ""%2"" >> Champ ""%3"" ", sLibelléVolet, sLibelléRegroupement, sLibelléChamp)
      			stUnElement.eType				= eltChamp
      			stUnElement.nIndiceVolet 		= nIndiceVolet
      			stUnElement.sRegroupementParent	= {sNomRegroupement, indChamp}..NomComplet
      			Ajoute(gtabElementsRuban, stUnElement)
      			
      			// Champ suivant
      			sNomChamp = EnumèreSousElément(gsNomChampRuban+"."+nIndiceVolet+"."+sNomRegroupement, enumVolet)	
      			
      		FIN
      		
      		CONTINUER :
      		// Regroupement suivant
      		sNomRegroupement = EnumèreSousElément(gsNomChampRuban+"."+nIndiceVolet, enumVolet)	
      		
      	FIN
      	
      FIN
      
      //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // Supprime les RC des libellés utilisés dans la zone répétée (remplacés par un espace)
      POUR TOUT ELEMENT stUnElement DE gtabElementsRuban
      	stUnElement.sBulleAide 		= chaineformate(Remplace(stUnElement.sBulleAide, RC, " "), 			ccsansespace)
      	stUnElement.sLibellé 			= chaîneFormate(Remplace(stUnElement.sLibellé, RC, " "), 			ccSansEspace)
      	stUnElement.sCheminAccesRuban = chaîneFormate(Remplace(stUnElement.sCheminAccesRuban, RC, " "), 	ccSansEspace)
      FIN
      
      //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // Positionne les champs de recherche en hauteur en fonction du ruban 
      IMG_FondRecherche..Y 			= {gsNomChampRuban, indchamp}..Y + {gsNomChampRuban, indChamp}..hauteur
      SAI_RechercheCommandeRuban..Y 	= IMG_FondRecherche..Y + 2
      BTN_FermeRechercheRuban..Y 		= IMG_FondRecherche..Y + 2
      ZR_ResultatRecherche..Y 		= IMG_FondRecherche..Y + 27
      
      //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // Mémorise l'image du ruban (chemin fichier)
      gsImageVoletInitial 			= {gsNomChampRuban, indchamp}..image
      // Charge dans une variable Image
      MonImage est une image 			= gsImageVoletInitial
      // Affiche l'image initiale dans un champ
      IMG_ImageVolet 					= gsImageVoletInitial
      // Positionne les dimensions des champs temporaires
      // (ces champs permettront de créer des images de volets en fonction de couleurs données pour mettre en évidence le volet lors de la recherche)
      IMG_ImageVolet..Largeur 		= max(1, MonImage ..Largeur)
      IMG_ImageVolet..Hauteur 		= max(1, MonImage ..Hauteur)
      IMG_ImageVoletFinal..Largeur 	= max(1, MonImage..Largeur)
      IMG_ImageVoletFinal..Hauteur 	= max(1, MonImage..Hauteur)
      
      
      // Chargement effectué
      renvoyer vrai
     type : 458752
   -
     name : _RechercheRuban_ElementEstVisible
     internal_properties : 
     procedure_id : 1269831497953781462
     type_code : 14
     code : |1+
      // Résumé : Détermine si un élément est visible ou non
      // Syntaxe :
      //[ <Résultat> = ] _RechercheRuban_ElementEstVisible (<stUnElement> est STElementRuban)
      //
      // Paramètres :
      //	stUnElement (STElementRuban) : Nom complet de l'élément
      // Valeur de retour :
      // 	booléen : Vrai si l'élément est visible, Faux autrement
      //// Traitement automatique des erreurs : RENVOYER FAUX
      //
      
      PROCEDURE PRIVEE _RechercheRuban_ElementEstVisible(LOCAL stUnElement est un STElementRuban) : booleen
      
      // Quel que soit le type de l'élément, il faut que le volet soit visible
      SI {gsNomChampRuban, indChamp}[stUnElement.nIndiceVolet]..Visible = Faux ALORS RENVOYER Faux
      
      quand exception dans
      	
      	// Selon le type de l'élément
      	selon stUnElement.eType
      		// Volet
      		CAS eltVolet
      			// Le cas du volet invisible est traité sur la première ligne de code
      			renvoyer vrai
      			
      		// Regroupement
      		CAS eltregroupement			
      			// Renvoie	la visibilité du regroupement 
      			renvoyer {stUnElement.sNomCompletChamp, indChamp}..Visible
      		
      		// Champ
      		CAS eltchamp
      			// Il faut que le regroupement soit visible
      			si {stUnElement.sregroupementparent, indchamp}..visible = faux alors renvoyer faux
      				
      			// Renvoie	la visibilité du champ 
      			RENVOYER {stUnElement.sNomCompletChamp, indChamp}..Visible
      		
      		// Inconnu
      		autres cas
      		dbgassertion(faux, "Type d'élément inconnu")
      	
      	FIN
      	
      	// Si rien n'est trouvé, renvoie Faux
      	renvoyer faux
      	
      FAIRE
      	renvoyer faux
      FIN
     type : 458752

     name : _RechercheRuban_SurligneTexte
     internal_properties : CAAAAAgAAAAV2sXBrX7kjgWEvzbFSNsNsm1w2R4Vzkc9YQWwkJwywRZGma6eXyAgcU2lIL1sl+kRe+l0cSKIjDdhosCEbZZD3TLbM1/VuQSIMcedINk0h7upSNsiUYnCudOzTlRJgqFUIrWUvpvsjs/LnIHx51O/0Ql23uQwP7mcSTRa4k4F3+n32bNEix+lOC2RZRHipN2jd7DhlWwBB35rOaPyvyHqapK6cflFwbL8z0i3CArjRgyj9mitErGcR95s5fyEDYl7Y0cGoyDPPT6N+TnUien53QeiBeLp
     procedure_id : 1268751055997264201
     type_code : 14
     code : |1-
      // Résumé : Formate un texte pour surligner une suite de caractères
      // Syntaxe :
      //[ <Résultat> = ] _RechercheRuban_SurligneTexte (<sTexteInitial> est chaîne, <sTermeRecherche> est chaîne, <sChampRéférence> est chaîne)
      //
      // Paramètres :
      //	sTexteInitial (chaîne ANSI) : Texte initial
      //	sTermeRecherche (chaîne ANSI) : Terme recherché à mettre en évidence dans le texte initial
      //	sChampRéférence (chaîne ANSI) : Champ de référence pour récupérer la police initiale
      // Valeur de retour :
      // 	chaîne ANSI : Texte formaté
      //// Traitement automatique des erreurs : RENVOYER Chaine vide "
      //
      
      PROCEDURE PRIVEE _RechercheRuban_SurligneTexte(LOCAL sTexteInitial est une chaîne, LOCAL sTermeRecherche est une chaine, LOCAL sChampRéférence est une chaine)
      
      sTexteFinal	est une chaine
      nPosDeb		est un entier
      
      
      // Mémorise le texte initial
      sTexteFinal = sTexteInitial
      
      // Recherche chaque occurrence du terme
      nPosDeb = position(sTexteFinal, sTermeRecherche, 1, SansCasse)
      si nPosDeb > 0
      
      	// Formate cette partie de texte
      	sTexteFinal = rtfselection(sTexteFinal, rtfCouleurFond, JauneClair, nPosDeb, Taille(sTermeRecherche))
      
      FIN
      
      // Applique la police (nom et taille) du champ référence
      sTexteFinal = RTFSélection(sTexteFinal, rtfNomPolice, 		{sChampRéférence, indChamp}..PoliceNom, 	1, Taille(sTexteFinal))
      sTexteFinal = RTFSélection(sTexteFinal, rtfTaillePolice, 	{sChampRéférence, indChamp}..PoliceTaille, 	1, Taille(sTexteFinal))
      
      
      // Texte formaté
      renvoyer sTexteFinal
